{
  "request": {
    "method": "POST",
    "urlPath": "/login-password/input",
    "queryParameters": {
      "r_url": {
        "matches": "^.+$"
      }
    }
  },
  "response": {
    "status": 200,
    "bodyFileName": "webview/login_password.html",
    "headers": {
      "Content-Type": "text/html; charset=UTF-8"
    },
    "transformers": [
      "response-template"
    ]
  }
}