{
  "request": {
    "method": "GET",
    "urlPathPattern": "/front/category/([0-9]{5})",
    "queryParameters": {
      "category_type": {
        "matches": "^[a-z]+$"
      }
    }
  },
  "response": {
    "status": 200,
    "bodyFileName": "front/get_front_category_shop_code_200_{{request.path.[2]}}.json",
    "headers": {
      "Content-Type": "application/json; charset=UTF-8",
      "Set-Cookie": "X-IYNS-LOGIN-TOKEN=wiremock-test"
    },
    "transformers": [
      "response-template"
    ]
  }
}