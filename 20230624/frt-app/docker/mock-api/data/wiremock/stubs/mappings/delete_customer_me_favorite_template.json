    "queryParameters": {
      "cart_id": {
        "matches": "^[1-9][0-9]{0,7}$"
      },
      "product_ids": {
        "matches": "^[0-9,]{1,200}$"
      }
    }
  },
  "response": {
    "status": 200,
    "bodyFileName": "favorite/delete_customer_me_favorite_200_normal.json",
    "headers": {
      "Content-Type": "application/json; charset=UTF-8"
    },
    "transformers": [
      "response-template"
    ]
  }
}