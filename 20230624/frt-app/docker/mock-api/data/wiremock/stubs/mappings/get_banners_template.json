{
  "request": {
    "method": "GET",
    "urlPath": "/banners",
    "queryParameters": {
      "shop_code": {
        "matches": "^[0-9]{5}$"
      },
      "product_count": {
        "matches": "^[0-9]{1,2}$"
      },
      "banner_type": {
        "matches": "^[1-3]{1}$"
      },
      "front_div": {
        "matches": "^[1-3]{1}$"
      },
      "cart_id": {
        "matches": "^[1-9][0-9]{0,7}$"
      }
    }
  },
  "response": {
    "status": 200,
    "bodyFileName": "banners/get_banners_200_normal.json",
    "headers": {
      "Content-Type": "application/json; charset=UTF-8"
    },
    "transformers": [
      "response-template"
    ]
  }
}